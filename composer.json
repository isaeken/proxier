{
  "name": "isaeken/proxier",
  "version": "1.1.0",
  "description": "Web proxy for PHP",
  "keywords": [
    "proxy",
    "web",
    "php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Isa Eken",
      "email": "hello@isaeken.com.tr"
    }
  ],
  "bin": "bin/proxier",
  "scripts": {
    "start": "php -S 127.0.0.1:8000 -t ./examples/server",
    "proxier": "php bin/proxier",
    "test": "phpunit ./tests"
  },
  "require": {
    "php": "^8.2",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-dom": "*",
    "symfony/http-foundation": "^6.2",
    "guzzlehttp/guzzle": "^7.0",
    "minicli/minicli": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "IsaEken\\Proxier\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "IsaEken\\Proxier\\Tests\\": "tests/"
    }
  }
}
